{
  "manifest_version": 2,
  "name": "Some Nice Name",
  "description": "Some nice Description",
  "version": "0.1",
  "permissions": [
      "storage",
      "webRequest",
      "webRequestBlocking",
      "*://*.youtube.com/watch*"
  ],
  "content_scripts": [
      {
          "matches": [
              "*://*.youtube.com/watch*"
          ],
          "js": [
              "cs.js"
          ],
          "run_at": "document_end"
      }
  ],
  "background": {
    "scripts": ["bg.js"]
  },
  "browser_action": {
    "default_icon": {                    // optional
        "16": "assets/icon16.png"
    },
    "default_title": "Some Nice Name",      // optional; shown in tooltip
    "default_popup": "popup.html"        // optional
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}